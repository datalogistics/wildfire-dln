<menu buttonBg='bgs/button.xml' buttonWidth='90' buttonSpan='60' buttonRadius='65'>
            
    <button angle='-90' 
            icon='icons/obstruction_edit.png'
            submenu='menus/obs_vehicle_submenu.xml'
            disableSwap='true'
            swapIcon='false' />
                
    <button icon='icons/copy_ac.png' 
            onClick='actions/atsk/atsk_obs_copy.xml'/>
                
    <button icon='icons/info.png' 
            onClick='actions/atsk/atsk_obs_info.xml'/>
                
    <button icon='icons/pairing_line.png' 
            onClick='actions/pairingline.xml' 
            selected='${pairingline_on}'/>
                
    <button icon='icons/blast_rings.png' 
            onClick='actions/atsk/atsk_ac_rings.xml' 
            selected='${ac_rings_selected}' 
            disabled='!{${ac_rings}}'/>
                
    <button icon='icons/delete.png' 
            disabled='!{${removable}}' 
            onClick='actions/atsk/atsk_delete_obs.xml'/>
    
</menu>