<?xml version="1.0" encoding="utf-8"?>
<com.atakmap.android.tools.ActionBarView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:layout_marginBottom="0dp">

    <ImageButton
        android:id="@+id/atsk_toolbar_image_map"
        android:padding="2dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_gray"
        android:scaleType="fitCenter"
        android:src="@drawable/atsk_toolbar_map"/>

    <ImageButton
        android:id="@+id/atsk_toolbar_image_lrf"
        android:padding="2dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_gray"
        android:scaleType="fitCenter"
        android:src="@drawable/atsk_toolbar_lrf"/>

    <ImageButton
        android:id="@+id/atsk_toolbar_image_gps"
        android:padding="2dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_gray"
        android:scaleType="fitCenter"
        android:src="@drawable/atsk_toolbar_gps"/>

    <ImageButton
        android:id="@+id/atsk_toolbar_image_height"
        android:padding="2dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_gray"
        android:scaleType="fitCenter"
        android:src="@drawable/atsk_toolbar_height_bottom"/>

</com.atakmap.android.tools.ActionBarView>

